@inherits BlazorView<MenuItemControl>
<FluentMenuButton @onclick="@OnClick">@Title</FluentMenuButton>
@code
{
    private string Title { get; set; }
    protected override void OnParametersSet()
    {
        base.OnParametersSet();
        if (ViewModel != null)
        {
            DataBind<string>(ViewModel.Title, x => Title = x);
        }

    }

}
